{
  "name": "builder",
  "version": "2.0",
  "scripts": {
    "ghpages": "git subtree push --prefix build origin gh-pages",
    "build:dev": "gulp build",
    "build:prod": "gulp build --production",
    "start:dev": "gulp --open",
    "start:prod": "gulp --open --production",
    "precommit": "npm run lint",
    "lint:ts": "eslint --ext \".ts\" --ignore-path .gitignore .",
    "lint:ts:fix": "eslint \"**/*.ts\" --fix --color",
    "lint:js": "eslint --ext \".js\" --ignore-path .gitignore .",
    "lint:js:fix": "eslint \"**/*.js\" --fix --color",
    "lint:prettier": "prettier --check .",
    "lint:prettier:fix": "prettier . --write",
    "stylelint": "npx stylelint \"**/*.scss\"",
    "stylelint:fix": "stylelint \"**/*.scss\" --fix --color",
    "lint": "npm run lint:ts && npm run lint:js && npm run lint:prettier && npm run stylelint",
    "lintfix": "prettier --write --list-different . && npm run lint:ts:fix && npm run lint:js:fix && npm run stylelint:fix"
  },
  "authors": "Cadesign",
  "devDependencies": {
    "@babel/cli": "^7.21.5",
    "@babel/core": "^7.21.8",
    "@babel/eslint-parser": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-typescript": "^7.21.5",
    "@babel/register": "^7.21.0",
    "@babel/runtime-corejs2": "^7.21.5",
    "@babel/runtime-corejs3": "^7.21.5",
    "@fullhuman/postcss-purgecss": "^5.0.0",
    "@lipemat/css-mqpacker": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^8.3.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "browser-sync": "^2.29.1",
    "concurrently": "^8.0.1",
    "core-js": "^3.30.1",
    "del": "^6.1.1",
    "es5-ext": "0.10.53",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "glslify-loader": "^2.0.0",
    "gulp": "^4.0.2",
    "gulp-changed": "^3.2.0",
    "gulp-cheerio": "^0.6.3",
    "gulp-consolidate": "^0.2.0",
    "gulp-filter": "^5.1.0",
    "gulp-front-matter": "^1.3.0",
    "gulp-git": "^2.10.1",
    "gulp-if": "^2.0.2",
    "gulp-imagemin": "^7.1.0",
    "gulp-include": "^2.4.1",
    "gulp-notify": "^3.2.0",
    "gulp-nunjucks-render": "^2.2.3",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^9.0.1",
    "gulp-prettify": "^0.5.0",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^5.1.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-svgmin": "^1.2.4",
    "gulp-svgstore": "^7.0.1",
    "gulp-ttf2woff": "^1.1.1",
    "gulp-ttf2woff2": "^4.0.1",
    "gulp-uglify": "^3.0.2",
    "gulp-util": "^3.0.8",
    "gulp-version-number": "^0.2.4",
    "htmlhint": "^1.1.4",
    "husky": "^1.3.1",
    "imagemin-webp": "^6.1.0",
    "jquery": "^3.6.4",
    "lint-staged": "^13.2.2",
    "lodash": "^4.17.21",
    "optimize-plugin": "^1.3.1",
    "postcss": "^8.4.23",
    "postcss-csso": "^6.0.1",
    "prettier": "^3.1.0",
    "raw-loader": "^4.0.2",
    "require-dir": "^1.2.0",
    "require-yaml": "^0.0.1",
    "run-sequence": "^2.2.1",
    "sass": "^1.62.1",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "stylelint-order": "^6.0.3",
    "stylelint-prettier": "^3.0.0",
    "stylelint-scss": "^5.3.1",
    "stylelint-selector-bem-pattern": "^3.0.1",
    "through2": "^3.0.2",
    "ts-loader": "^9.4.2",
    "typescript": "5.1",
    "vanilla-lazyload": "^17.8.3",
    "webpack": "^5.81.0",
    "webpack-bundle-analyzer": "^4.8.0",
    "webpack-cli": "^5.0.2"
  },
  "dependencies": {
    "imask": "^6.6.1",
    "swiper": "^8.4.7",
    "webpack-dev-server": "^4.13.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*": "npm run lint"
  }
}
